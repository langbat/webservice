                <line2 xsi:type="xsd:string">APT 1A</line2>
                <city xsi:type="xsd:string">NEW YORK</city>
                <state xsi:type="xsd:string">NY</state>
                <zipcode xsi:type="xsd:string">10001</zipcode>
            </address>
            <homePhone xsi:type="xsd:string">2125551234</homePhone>
            <mobilePhone xsi:type="xsd:string">2125551234</mobilePhone>
            <workPhone xsi:type="xsd:string"></workPhone>
            <email xsi:type="xsd:string"></email>
        </x:createClient>
    </soapenv:Body>
</soapenv:Envelope>

